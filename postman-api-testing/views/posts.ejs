<!DOCTYPE html>
<html>
    <head>
        <title>Post List</title>
    </head>
    <body>
        <h1>Post List</h1>
        <ul>
            <% posts.forEach(function(post) { %>
                <li>
                    <h2><%= post.title %></h2>
                    <p><%= post.body %></p>
                </li>
                <hr>
            <% }); %>
        </ul>
    </body>
</html>
